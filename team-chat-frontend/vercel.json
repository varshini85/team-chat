{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/(.*)\\.(?:png|jpg|jpeg|gif|svg|css|js|map|ico)", "headers": { "cache-control": "public, max-age=31536000, immutable" }, "continue": true },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
